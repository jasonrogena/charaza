<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView
		xmlns:tools="http://schemas.android.com/tools"
		android:id="@+id/latestScrollView"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true">
		<RelativeLayout 
		    android:id="@+id/latestRelativeLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@color/layoutColor">
			<TableLayout 
			    android:id="@+id/latestTableLayout"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:stretchColumns="0"
			    android:layout_alignParentTop="true" >
			    <TableRow 
			        android:layout_height="36dp"
			        android:layout_width="fill_parent">
			        <TextView 
			            android:id="@+id/latestNameHeading"
			            android:layout_width="wrap_content"
			            android:layout_height="36dp"
			            android:gravity="center_vertical"
			            android:layout_marginLeft="4dp"
			            android:textSize="15dp"
			            android:textStyle="bold"
			            android:textColor="@color/normalTextColor"
			            android:text="@string/latestNameHeadingText"/>
			        <View 
			            android:id="@+id/latestTableHeadingSeparator"
			            android:layout_width="1dp"
			            android:layout_height="36dp"
			            android:background="@color/tableSeparatorColor"/>
			        <TextView 
			            android:id="@+id/latestTimeHeading"
			            android:layout_width="wrap_content"
			            android:layout_height="36dp"
			            android:gravity="center_vertical"
			            android:layout_marginLeft="3dp"
			            android:layout_marginRight="4dp"
			            android:textSize="15dp"
			            android:textStyle="bold"
			            android:textColor="@color/normalTextColor"
			            android:text="@string/latestTimeHeadingText"/>
			    </TableRow>
			    <TableRow 
			        android:layout_width="fill_parent"
			        android:layout_height="4dp" >
			        <View 
			            android:layout_height="1dp"
			            android:layout_width="fill_parent"
			            android:background="@color/tableSeparatorColor"
			            android:layout_span="3"/>
			    </TableRow>
			</TableLayout>
		</RelativeLayout>
	</ScrollView>
	<com.devspark.sidenavigation.SideNavigationView
		        android:id="@+id/side_navigation_view_latest"
		        android:layout_width="match_parent"
		        android:layout_height="match_parent" />
</RelativeLayout>